#include<stdio.h>
int main()
{
    int r1,c1,r2,c2,mat1,mat2,res;
    scanf("%d %d %d %d",&r1,&c1,&r2,&c2);
printf("Enter the matrix 1:");
for(int i=0;i<r1;i++){
    for(int j=0;j<c1;j++){
        scanf("%d",&mat1[i][j]);
    }
    
}
printf("Enter the matrix 2:");
for(int i=0;i<r2;i++)
{
    for(int j=0;j<c2;j++){
        scanf("%d",&mat2[i][j]);
    }
    for(int i=0;i<r2;i++)
    {
        for(int j=0;j<c2;j++){
            res[i][j]=mat1[i][j]+mat2[i][j];

        }

    }
    for(int i=0;i<r2;i++){
        for(int j=0;j<c2;j++){
            printf("%d",res);
        }
    }


    
}
return 0;
}
